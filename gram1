01) program = otherFunctions mainFunctions
02) mainFunctions = TK_MAIN stmts TK_END
03) otherFunctions = function otherFunctions
04) otherFunctions = epsilon
05) function = TK_FUNID input_par output_par TK_SEM stmts TK_END
06) input_par = TK_INPUT TK_PARAMETER TK_LIST TK_SQL parameter_list TK_SQR
07) output_par = TK_OUTPUT TK_PARAMETER TK_LIST TK_SQL parameter_list TK_SQR
08) output_par = epsilon
09) parameter_list = dataType TK_ID remaining_list
10) dataType = primitiveDatatype
11) dataType = constructedDatatype
12) primitiveDatatype = TK_INT
13) primitiveDatatype = TK_REAL
14) constructedDatatype = TK_RECORD TK_RECORDID
15) remaining_list = TK_COMMA parameter_list
16) remaining_list = epsilon
17) stmts = typeDefinitions declarations otherStmts returnStmt
18) typeDefinitions = typeDefinition typeDefinitions
19) typeDefinitions = epsilon
20) typeDefinition = TK_RECORD TK_RECORDID fieldDefinitions TK_ENDRECORD TK_SEM
21) fieldDefinitions = fieldDefinition fieldDefinitions moreFields
22) fieldDefinition = TK_TYPE primitiveDatatype TK_COLON TK_FIELDID TK_SEM
23) moreFields = fieldDefinition moreFields
24) moreFields = epsilon
25) declarations = declaration declarations
26) declarations = epsilon
27) declaration = TK_TYPE dataType TK_COLON TK_ID global_or_not TK_SEM
28) global_or_not = TK_COLON TK_GLOBAL
29) global_or_not = epsilon
30) otherStmts = stmt otherStmts
31) otherStmts = epsilon
32) stmt = assignmentStmt
33) stmt = iterativeStmt
34) stmt = conditionalStmt
35) stmt = ioStmt
36) stmt = funCallStmt
37) assignmentStmt = SingleOrRecId TK_ASSIGNOP arithmeticExpression TK_SEM
38) SingleOrRecId = TK_ID SingleOrRecId_
39) SingleOrRecId_ = TK_DOT TK_FIELDID
40) SingleOrRecId_ = epsilon
41) funCallStmt = outputParameters TK_CALL TK_FUNID TK_WITH TK_PARAMETERS inputParameters TK_SEM
42) outputParameters = TK_SQL idList TK_SQR TK_ASSIGNOP
43) outputParameters = epsilon
44) inputParameters = TK_SQL idList TK_SQR
45) iterativeStmt = TK_WHILE TK_OP booleanExpression TK_CL stmt otherStmts TK_ENDWHILE
46) conditionalStmt = TK_IF TK_OP booleanExpression TK_CL TK_THEN stmt otherStmts elseStmt
47) elseStmt = TK_ELSE stmt otherStmts TK_ENDIF
48) elseStmt = TK_ENDIF
49) ioStmt = TK_READ TK_OP SingleOrRecId TK_CL TK_SEM
50) ioStmt = TK_WRITE TK_OP allVar TK_CL TK_SEM
51) allVar = var
52) allVar = TK_RECORDID
53) arithmeticExpression = term innerTerm
54) innerTerm = plusMinus term innerTerm
55) innerTerm = epsilon
56) term = factor termPrime
57) termPrime = mulDiv factor termPrime
58) termPrime = epsilon
59) factor = TK_OP arithmeticExpression TK_CL
60) factor = all
61) mulDiv = TK_MUL
62) mulDiv = TK_DIV
63) plusMinus = TK_PLUS
64) plusMinus = TK_MINUS
65) all = TK_ID
66) all = TK_NUM
67) all = TK_RNUM
68) all = TK_RECORDID recAdjust
69) recAdjust = epsilon
70) recAdjust = TK_DOT TK_FIELDID
71) booleanExpression = TK_OP booleanExpression TK_CL logicalOp TK_OP booleanExpression TK_CL
72) booleanExpression = var relationalOp var
73) booleanExpression = TK_NOT booleanExpression
74) var = TK_ID
75) var = TK_NUM
76) var = TK_RNUM
77) logicalOp = TK_AND
78) logicalOp = TK_OR
79) relationalOp = TK_LT
80) relationalOp = TK_LE
81) relationalOp = TK_EQ
82) relationalOp = TK_GT
83) relationalOp = TK_GE
84) relationalOp = TK_NE
85) returnStmt = TK_RETURN optionalReturn TK_SEM
86) optionalReturn = TK_SQL idList TK_SQR
87) optionalReturn = epsilon
88) idList = TK_ID more_ids
89) more_ids = TK_COMMA idList
90) more_ids = epsilon
